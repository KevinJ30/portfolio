gin-bottom: 16px;\n  line-height: 1.5em;\n}\n\n.skills__container {\n  display: flex;\n  flex-direction: row;\n  margin-bottom: 32px;\n}\n\n.skills__container > .skill {\n  margin-right: 32px;\n}\n\n.skills__container > .skill:last-child {\n  margin-right: 0;\n}\n\n.section_skills h3 {\n  font-size: 1.8em;\n  margin-bottom: 32px;\n}\n\n.skill {\n  display: inline-flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.skill > div {\n  margin-bottom: 16px;\n  font-weight: 400;\n  text-transform: uppercase;\n}\n\n.skill > div:last-child {\n  margin-bottom: 0;\n}\n\n.skill__notes {\n  display: flex;\n  flex-direction: row;\n}\n\n.skill__notes > span {\n  margin-right: 8px;\n}\n\n.skill_notes > span:last-child {\n  margin-right: 0;\n}\n\n.skill__svg svg {\n  width: 80px;\n  height: 80px;\n}\n\n.projects__container {\n  display: grid;\n  margin-top: 48px;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 411px));\n  grid-gap: 48px;\n  justify-content: center;\n}\n\n@media (max-width: 1080px) {\n  .section_home .grid-2 {\n    grid-template-columns: repeat(1, 1fr);\n  }\n\n  .section_home {\n    text-align: center;\n  }\n\n  .section_home .introduction_image {\n    order: -1;\n  }\n\n  .projects__container {\n    grid-template-columns: repeat(2, 1fr);\n  }\n}\n@media (max-width: 800px) {\n  .projects__container {\n    grid-template-columns: repeat(1, 1fr);\n  }\n}\n@media (max-width: 600px) {\n  .section__title {\n    font-size: 2.2em;\n  }\n\n  .section_home .introduction__callActions {\n    flex-direction: column;\n  }\n\n  .section_home .introduction__callActions > .btn {\n    margin: 0 0 16px;\n  }\n\n  .section_skills h3 {\n    text-align: center;\n  }\n\n  .skills__container {\n    flex-direction: column;\n  }\n\n  .skills__container > .skill {\n    margin-right: 0;\n    margin-bottom: 32px;\n  }\n\n  .skills__container > .skill:last-child {\n    margin-right: 0;\n    margin-bottom: 0;\n  }\n}\n/**\n * Pages Administration\n **/\n.page-admin_skills ta