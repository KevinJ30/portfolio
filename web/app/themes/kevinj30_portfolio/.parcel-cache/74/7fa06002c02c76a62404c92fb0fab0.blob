ng, sub, sup, tt, var,\nb, u, i, center,\ndl, dt, dd, menu, ol, ul, li,\nfieldset, form, label, legend,\ntable, caption, tbody, tfoot, thead, tr, th, td,\narticle, aside, canvas, details, embed,\nfigure, figcaption, footer, header, hgroup,\nmain, menu, nav, output, ruby, section, summary,\ntime, mark, audio, video {\n  margin: 0;\n  padding: 0;\n  border: 0;\n  font-size: 100%;\n  font: inherit;\n  vertical-align: baseline;\n}\n\n/* HTML5 display-role reset for older browsers */\narticle, aside, details, figcaption, figure,\nfooter, header, hgroup, main, menu, nav, section {\n  display: block;\n}\n\n/* HTML5 hidden-attribute fix for newer browsers */\n*[hidden] {\n  display: none;\n}\n\nbody {\n  line-height: 1;\n}\n\nmenu, ol, ul {\n  list-style: none;\n}\n\nblockquote, q {\n  quotes: none;\n}\n\nblockquote:before, blockquote:after,\nq:before, q:after {\n  content: \"\";\n  content: none;\n}\n\ntable {\n  border-collapse: collapse;\n  border-spacing: 0;\n}\n\n/**\n * Tools\n **/\n/**\n * Color\n **/\n/**\n * Fonts\n **/\n.visually-hidden {\n  position: absolute !important;\n  border: 0 !important;\n  height: 1px !important;\n  width: 1px !important;\n  padding: 0 !important;\n  overflow: hidden !important;\n  clip: rect(0, 0, 0, 0) !important;\n}\n\n/**\n * Heading\n **/\nh1 {\n  margin-bottom: 16px;\n  font-size: 1.8em;\n}\n\nh2 {\n  margin-bottom: 16px;\n  font-size: 1.6em;\n}\n\nh3 {\n  margin-bottom: 16px;\n  font-size: 1.4em;\n}\n\nh4 {\n  margin-bottom: 16px;\n  font-size: 1.2em;\n}\n\nh5 {\n  margin-bottom: 16px;\n  font-size: 1em;\n}\n\nh1, h2, h3, h4, h5 {\n  font-family: \"Rochester\", cursive;\n  font-weight: 400;\n}\n\nul {\n  list-style: disc inside;\n}\n\nul li {\n  padding: 8px 0;\n}\n\na {\n  border-bottom: 1px solid #EFEFF0;\n  text-decoration: none;\n  color: #EFEFF0;\n}\n\n.text-align-center {\n  text-align: center;\n}\n\n.container {\n  margin: auto;\n  max-width: 80%;\n}\n\n.grid {\n  display: grid;\n}\n\n.grid.grid-2 {\n  grid-template-columns: repeat(2, 1fr);\n}\n\n.grid-gap-1 {\n  grid-gap: 8px;\n}\n\n.grid-gap-2 {\n  grid-gap: 16px;\n}\n\n.grid-gap-3 {\n  grid-gap: 24px;\n}\n\n.grid-gap-4 {\n  grid-gap: 32px;\n}\n\n.align-items-center {\n  align-items: center;\n}\n\n.mt-2 {\n  margin-top: 16px !important;\n}\n\n.mt-4 {\n  margin-top: 32px !important;\n}\n\n.mt-5 {\n  margin-top: 40px !important;\n}\n\n.mb-2 {\n  margin-bottom: 16px !important;\n}\n\n.mb-5 {\n  margin-bottom: 40px !important;\n}\n\n.pb-5 {\n  padding-bottom: 40px;\n}\n\n/**\n * Components\n **/\n.icon svg {\n  display: inline-block;\n  height: 18px;\n  width: 18px;\n}\n\n.icon .icon-stars {\n  color: #131618;\n}\n\n.icon.icon-stars.active {\n  color: #a66737;\n}\n\n.btn {\n  display: inline-block;\n  padding: 11px 21px;\n  border-radius: 3px;\n  text-decoration: none;\n  border: 1px solid #EFEFF0;\n  color: #EFEFF0;\n  cursor: pointer;\n  font-weight: 600;\n  transition: 0.3s ease;\n}\n\n.btn:hover {\n  background: rgba(239, 239, 240, 0.08);\n}\n\n.btn-primary {\n  background: #C2B787;\n  border-color: #C2B787;\n  color: #131618;\n}\n\n.btn-primary:hover {\n  background: #d3cba9;\n}\n\n.btn-primary.btn-outline:hover {\n  background: rgba(194, 183, 135, 0.08);\n}\n\n.btn-primary.btn-outline {\n  background: transparent;\n  color: #C2B787;\n}\n\n.card {\n  position: relative;\n  background: #282A2B;\n  border-radius: 5px 5px 0 0;\n  display: inline-flex;\n  flex-direction: column;\n  box-shadow: 0 2px 0 0 #C2B787;\n  transition: box-shadow 0.3s ease;\n}\n\n.card:hover {\n  box-shadow: 0 4px 0 0 #C2B787;\n}\n\n.card__image {\n  height: 300px;\n  overflow: hidden;\n  border-radius: 5px 5px 0 0;\n}\n\n.card__image img {\n  width: 100%;\n  height: 300px;\n  border-radius: 5px 5px 0 0;\n  object-fit: cover;\n  transition: transform 0.4s ease;\n}\n\n.card__image:hover img {\n  transform: scale(1.1);\n}\n\n.card__image:after {\n  position: absolute;\n  content: \"\";\n  display: block;\n  width: 100%;\n  height: 300px;\n  left: 0;\n  top: 0;\n  background: rgba(0, 0, 0, 0.4);\n  border-radius: 5px 5px 0 0;\n}\n\n.card__header {\n  font-size: 1.8em;\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translateX(-50%) translateY(calc(-50% - 100px));\n  z-index: 1000;\n}\n\n.card__content {\n  padding: 32px;\n}\n\n.card__details {\n  margin-bottom: 24px;\n  color: #C2B787;\n  font-size: 0.8em;\n}\n\n.form-group {\n  display: flex;\n  flex-direction: column;\n  padding-bottom: 16px;\n}\n\n.form-group > label {\n  font-weight: 400;\n  margin-bottom: 8px;\n}\n\n.form-control {\n  display: block;\n  padding: 8px 12px;\n  width: 100%;\n  border-radius: 5px;\n  background: inherit;\n  border: 1px solid #EFEFF0;\n  color: #EFEFF0;\n}\n\n.modal {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100vh;\n  background: rgba(0, 0, 0, 0.5);\n  overflow: hidden;\n}\n\n.modal--wrapper {\n  position: relative;\n  padding: 16px;\n  width: 80%;\n  background: #EFEFF0;\n  border-radius: 5px;\n  box-shadow: 10px 10px 40px #0F1315;\n  animation: slideInTop 1s;\n  transition: transform 0.5s, opacity 0.5s;\n  transition-timing-function: ease-in-out;\n}\n\n.modal--wrapper > * {\n  color: #000;\n}\n\n.modal-button-close {\n  position: absolute;\n  top: 16px;\n  right: 16px;\n  border: none;\n  cursor: pointer;\n}\n\n.modal-button-close svg {\n  color: rgba(19, 22, 24, 0.5);\n  transition: color 0.3s ease, transform 0.3s ease;\n}\n\n.modal-button-close:hover svg {\n  color: #131618;\n  transform: translateY(-1px) scale(1.03);\n}\n\n.modal-header {\n  font-size: 1.4em;\n}\n\n.modal--wrapper label {\n  font-weight: 400;\n}\n\n.modal--wrapper input {\n  border-color: #131618;\n  color: #131618;\n}\n\n@keyframes slideInTop {\n  0% {\n    transform: translateY(-10px);\n    opacity: 0;\n  }\n  100% {\n    transform: translateY(0);\n    opacity: 1;\n  }\n}\n* {\n  box-sizing: border-box;\n}\n\nhtml {\n  scroll